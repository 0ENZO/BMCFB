{% extends 'base.html.twig' %}
{% block title %}{{ parent() }} - Administration {% endblock %}

{% block body %}

    <div class="container-fluid">
        {% if records|length < 1 %}
            {{ include('questionnaire/not_started.html.twig') }}
        {% elseif finished and records|length > 1 %}
            {{ include('questionnaire/finished.html.twig') }}
        {% else %}
            {{ include('questionnaire/in_progress.html.twig') }}
        {% endif %}
    </div>

{% endblock %}
